{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./build/",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true /* Disallow inconsistently-cased references to the same file. */
  },
  "include": [
    "app",
    "app/src/endpoints/image/PostImage/.svn"  ],
  "exclude": [
    "node_modules",
    "build"
  ]
}